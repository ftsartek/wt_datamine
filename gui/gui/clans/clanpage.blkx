{
  "animated_wait_icon": {
    "background-rotation": "0",
    "id": "clan_loading",
    "pos": "0.5pw-0.5w, 0.5ph-0.5h",
    "position": "absolute"
  },
  "flow": "h-flow",
  "id": "clan_container",
  "inlineHolder": {
    "Button_text": [
      {
        "buttonWink": {},
        "display": "hide",
        "id": "btn_showRequests",
        "on_click": "onShowRequests",
        "text": "#clan/btnShowRequests",
        "visualStyle": "secondary",
        "wink": "no"
      },
      {
        "display": "hide",
        "id": "btn_clanActivity",
        "on_click": "onClanAverageActivity",
        "showConsoleImage": "no",
        "text": "#clan/activity",
        "visualStyle": "noFrame"
      },
      {
        "display": "hide",
        "id": "btn_clanVehicles",
        "on_click": "onClanVehicles",
        "showConsoleImage": "no",
        "text": "#clan/vehicles",
        "visualStyle": "noFrame"
      },
      {
        "display": "hide",
        "id": "btn_clanSquads",
        "on_click": "onClanSquads",
        "showConsoleImage": "no",
        "text": "#clan/clan_squads",
        "visualStyle": "noFrame"
      },
      {
        "display": "hide",
        "id": "btn_log",
        "on_click": "onClanLog",
        "showConsoleImage": "no",
        "text": "#clan/clan_log",
        "visualStyle": "noFrame"
      }
    ],
    "bgcolor": "@evenTrColor",
    "css-hier-invalidate": "yes",
    "display": "hide",
    "enable": "no",
    "flow": "h-flow",
    "flow-align": "center",
    "foreground-color": "@frameBorderColor",
    "foreground-position": "0, 0, 0, h-1@dp",
    "id": "clan_actions",
    "position": "relative",
    "re-type": "9rect",
    "width": "pw"
  },
  "navBar": {
    "class": "embedded",
    "navLeft": {
      "Button_text": [
        {
          "ButtonImg": {},
          "btnName": "LT",
          "display": "hide",
          "id": "btn_complain",
          "on_click": "onClanComplain",
          "text": "#mainmenu/btnComplain"
        },
        {
          "ButtonImg": {},
          "btnName": "LT",
          "buttonWink": {},
          "display": "hide",
          "id": "btn_membership_req",
          "on_click": "onMembershipReq",
          "text": "#clan/btn_membership_req",
          "visualStyle": "secondary"
        },
        {
          "display": "hide",
          "id": "btn_leaveClan",
          "on_click": "onLeaveClan",
          "text": "#clan/btnLeaveClan"
        },
        {
          "ButtonImg": {},
          "btnName": "LB",
          "display": "hide",
          "id": "btn_edit_clan_info",
          "on_click": "onEditClanInfo",
          "text": "#msgbox/btn_edit"
        },
        {
          "ButtonImg": {},
          "_on_click": "onChangeMembershipRequirementsWnd",
          "btnName": "RB",
          "display": "hide",
          "id": "btn_clan_membership_req_edit",
          "text": "#clan/btnChangeMembershipRequirements"
        },
        {
          "ButtonImg": {},
          "display": "hide",
          "id": "btn_showBlacklist",
          "on_click": "onOpenClanBlacklist",
          "text": "#clan/blacklist"
        },
        {
          "ButtonImg": {},
          "btnName": "R3",
          "buttonWink": {},
          "display": "hide",
          "id": "btn_upgrade_clan",
          "on_click": "onUpgradeClan",
          "text": "#clan/clan_upgrade",
          "visualStyle": "secondary"
        },
        {
          "ButtonImg": {},
          "btnName": "A",
          "display": "hide",
          "id": "btn_usercard",
          "on_click": "onUserCard",
          "skip-navigation": "yes",
          "text": "#mainmenu/btnUserCard"
        },
        {
          "ButtonImg": {},
          "_on_click": "onUserOption",
          "btnName": "X",
          "display": "hide",
          "id": "btn_user_options",
          "skip-navigation": "yes",
          "text": "#mainmenu/btnUserAction"
        }
      ]
    },
    "navRight": {
      "Button_text": {
        "ButtonImg": {},
        "btnName": "Y",
        "display": "hide",
        "id": "btn_season_reward_log",
        "on_click": "onClanSeasonRewardLog",
        "text": "#profile/awards"
      }
    }
  },
  "tdiv": [
    {
      "flow": "h-flow",
      "tdiv": {
        "tdiv": [
          {
            "background-color": "@separatorBlockColor",
            "img": {
              "background-image": "#ui/images/avatars/cardicon_default",
              "display": "hide",
              "height": "w",
              "id": "clan-icon",
              "max-height": "0.14@sf",
              "max-width": "0.14@sf",
              "width": "0.26pw"
            },
            "margin-right": "1@blockInterval",
            "padding": "1@blockInterval",
            "tdiv": {
              "flow": "h-flow",
              "tdiv": {
                "flow": "vertical",
                "padding": "1@blockInterval, 0",
                "tdiv": {
                  "tdiv": {
                    "Button_text": {
                      "ButtonImg": {},
                      "btnName": "L3",
                      "class": "image",
                      "display": "hide",
                      "id": "btn_lock_clan_req",
                      "img": {},
                      "on_click": "onLockNewReqests",
                      "tooltip": "#clan/open_close_new_req"
                    },
                    "clan_locked": "no",
                    "id": "nest_lock_clan_req",
                    "img": {
                      "background-color": "@white",
                      "background-svg-size": "@unlockStageIconSize, @unlockStageIconSize",
                      "display": "hide",
                      "id": "img_lock_clan_req",
                      "size": "@unlockStageIconSize, @unlockStageIconSize",
                      "tooltip": "#clan/closed_for_new_req"
                    },
                    "pos": "0, ph/2-h/2",
                    "position": "relative"
                  },
                  "textareaNoTab": {
                    "hideEmptyText": "yes",
                    "id": "clan-title",
                    "max-height": "0.2@sf",
                    "max-width": "pw",
                    "mediumFont": "yes",
                    "overflow-y": "none",
                    "overlayTextColor": "active",
                    "pare-text": "yes",
                    "pos": "0, ph/2-h/2",
                    "position": "relative",
                    "text": ""
                  },
                  "width": "pw"
                },
                "textareaNoTab": [
                  {
                    "id": "clan-memberCount-date",
                    "text": ""
                  },
                  {
                    "hideEmptyText": "yes",
                    "id": "clan-prevChanges",
                    "smallFont": "yes",
                    "text": ""
                  },
                  {
                    "hideEmptyText": "yes",
                    "id": "clan-region",
                    "max-height": "0.05@sf",
                    "overflow-y": "auto",
                    "smallFont": "yes",
                    "text": "",
                    "width": "pw"
                  },
                  {
                    "hideEmptyText": "yes",
                    "id": "clan-membershipReq",
                    "smallFont": "yes",
                    "text": ""
                  }
                ],
                "width": "pw"
              },
              "width": "fw"
            },
            "width": "0.55pw"
          },
          {
            "display": "hide",
            "flow": "h-flow",
            "id": "clan_main_stats",
            "table": {
              "class": "schemeTable",
              "id": "clan_stats_table",
              "width": "pw"
            },
            "textareaNoTab": {
              "font-bold": "@fontNormal",
              "id": "update_stats_info_text",
              "max-width": "pw",
              "overlayTextColor": "faded",
              "smallFont": "yes",
              "text": ""
            },
            "width": "fw"
          }
        ],
        "width": "pw"
      },
      "textarea": {
        "hideEmptyText": "yes",
        "id": "clan-about",
        "max-height": "0.1@sf",
        "overflow-y": "auto",
        "padding": "0, 0.01@sf",
        "smallFont": "yes",
        "text": "",
        "text-align": "left",
        "width": "pw"
      },
      "textareaNoTab": {
        "bgcolor": "@evenTrColor",
        "hideEmptyText": "yes",
        "id": "clan-motto",
        "overlayTextColor": "userlog",
        "pos": "0, 1@buttonMargin",
        "position": "relative",
        "text": "",
        "text-align": "center",
        "width": "pw"
      },
      "width": "pw"
    },
    {
      "HorizontalListBox": {
        "id": "modes_list",
        "move-only-hover": "yes",
        "navigatorShortcuts": "yes",
        "on_select": "cp_onStatsModeChange",
        "pos": "0, ph - h",
        "position": "absolute",
        "type": "transparent"
      },
      "div": {
        "behaviour": "button",
        "display": "hide",
        "id": "clan_awards_container",
        "max-width": "6@clanMedalSizeMin",
        "on_click": "onClanSeasonRewardLog",
        "overflow": "hidden",
        "padding": "0, 1@buttonMargin",
        "pos": "pw-w+1@blockInterval, 0",
        "position": "relative",
        "skip-navigation": "yes",
        "total-input-transparent": "yes"
      },
      "size": "pw, @clanMedalSizeMin + 2@buttonMargin",
      "tdiv": {
        "div": [
          {
            "cardImg": {
              "background-image": "#ui/gameuiskin#lb_elo_rating.svg",
              "id": "clan_elo_icon",
              "margin": "0.25@sIco",
              "type": "medium"
            },
            "textarea": {
              "caption": "yes",
              "id": "clan_elo_value",
              "input-transparent": "yes",
              "margin": "0.25@sIco",
              "overlayTextColor": "userlog",
              "removeParagraphIndent": "yes",
              "text": ""
            },
            "tooltip": "#clan/squadron_rating",
            "total-input-transparent": "yes"
          },
          {
            "cardImg": {
              "background-image": "#ui/gameuiskin#lb_activity.svg",
              "display": "hide",
              "id": "clan_activity_icon",
              "margin": "0.25@sIco",
              "type": "medium"
            },
            "textarea": {
              "caption": "yes",
              "display": "hide",
              "id": "clan_activity_value",
              "margin": "0.25@sIco",
              "overlayTextColor": "userlog",
              "removeParagraphIndent": "yes",
              "text": ""
            },
            "tooltip": "#clan/squadron_activity",
            "total-input-transparent": "yes"
          }
        ],
        "position": "absolute"
      }
    },
    {
      "id": "clan_members_list_nest",
      "overflow-y": "auto",
      "size": "pw, fh",
      "table": {
        "class": "lbTable",
        "id": "clan_members_list",
        "navigatorShortcuts": "yes",
        "on_dbl_click": "onUserCard",
        "on_r_click": "onUserRClick",
        "on_select": "onSelectUser",
        "on_set_focus": "onMembersListFocus",
        "padding-top": "1@blockInterval",
        "selfFocusBorder": "yes",
        "text-halign": "center",
        "text-valign": "center",
        "useNavigatorOrInteractiveCells": "yes",
        "width": "pw"
      }
    },
    {
      "id": "lb_table_nest",
      "padding-top": "1@blockInterval",
      "size": "pw, fh"
    }
  ],
  "textareaNoTab": {
    "display": "hide",
    "id": "season_over_notice",
    "padding": "0, 1@blockInterval",
    "pos": "50%pw-50%w, 0",
    "position": "relative",
    "smallFont": "yes",
    "text": "#worldwar/seasonIsOverNotice"
  }
}
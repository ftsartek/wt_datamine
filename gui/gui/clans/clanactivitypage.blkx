{
  "flow": "vertical",
  "id": "clan_activity_container",
  "padding": "0.01*@scrn_tgt",
  "tdiv": [
    {
      "left": "pw-w-0.02@scrn_tgt",
      "position": "relative",
      "tdiv": {
        "cardImg": {
          "background-image": "#ui/gameuiskin#lb_activity.svg",
          "id": "clan_activity_today_icon",
          "tooltip": "#clan/activity",
          "type": "medium"
        },
        "padding-left": "0.01@scrn_tgt",
        "textarea": {
          "caption": "yes",
          "id": "clan_activity_today_value",
          "overlayTextColor": "userlog",
          "position": "relative",
          "text": "",
          "top": "50%ph-50%h"
        }
      },
      "textareaNoTab": {
        "caption": "yes",
        "id": "clan_activity_today_text",
        "overlayTextColor": "active",
        "text": "#clan/activity_current"
      }
    },
    {
      "left": "pw-w-0.02@scrn_tgt",
      "position": "relative",
      "tdiv": {
        "cardImg": {
          "background-image": "#ui/gameuiskin#lb_activity.svg",
          "id": "clan_activity_total_icon",
          "tooltip": "#clan/activity",
          "type": "medium"
        },
        "padding-left": "0.01@scrn_tgt",
        "textarea": {
          "caption": "yes",
          "id": "clan_activity_total_value",
          "overlayTextColor": "userlog",
          "position": "relative",
          "text": "",
          "top": "50%ph-50%h"
        }
      },
      "textareaNoTab": {
        "caption": "yes",
        "id": "clan_activity_total_text",
        "overlayTextColor": "active",
        "text": "#clan/activity_total"
      }
    },
    {
      "table": {
        "class": "lbTable",
        "id": "clan_member_activity_history_table_header",
        "text-halign": "center",
        "text-valign": "center",
        "width": "fw"
      },
      "width": "pw"
    },
    {
      "overflow-y": "auto",
      "scrollbarShortcuts": "yes",
      "size": "pw, fh",
      "table": {
        "class": "lbTable",
        "id": "clan_member_activity_history_table",
        "text-halign": "center",
        "text-valign": "center",
        "width": "fw"
      }
    }
  ]
}
{
  "div": {
    "behavior": "Timer",
    "flow": "vertical",
    "id": "queue_box",
    "overflow-y": "auto",
    "padding": "4*@sf/@pf_outdated",
    "pos": "0,0",
    "position": "relative",
    "size": "pw, ph",
    "tdiv": [
      {
        "flow": "vertical",
        "frameBlock_dark": [
          {
            "activeText": {
              "id": "team_name",
              "pos": "0, 0",
              "position": "relative",
              "text": "#events/teamA"
            },
            "bgTeamColor": "blue",
            "css-hier-invalidate": "yes",
            "display": "hide",
            "flow": "vertical",
            "id": "1_block",
            "padding": "3*@sf/@pf_outdated",
            "pos": "0,0",
            "position": "relative",
            "table": {
              "id": "table_queue_stat",
              "smallFont": "yes",
              "width": "pw"
            },
            "tdiv": [
              {
                "id": "countries",
                "pos": "0, 0",
                "position": "relative",
                "width": "pw"
              },
              {
                "activeText": {
                  "id": "players_count",
                  "text": ""
                },
                "margin-left": "0.01*@sf"
              }
            ],
            "width": "pw"
          },
          {
            "activeText": {
              "id": "team_name",
              "pos": "0, 0",
              "position": "relative",
              "text": "#events/teamB"
            },
            "bgTeamColor": "red",
            "css-hier-invalidate": "yes",
            "display": "hide",
            "flow": "vertical",
            "id": "2_block",
            "padding": "3*@sf/@pf_outdated",
            "pos": "0, @blockInterval",
            "position": "relative",
            "table": {
              "id": "table_queue_stat",
              "smallFont": "yes",
              "width": "pw"
            },
            "tdiv": [
              {
                "id": "countries",
                "pos": "0, 0",
                "position": "relative",
                "width": "pw"
              },
              {
                "activeText": {
                  "id": "players_count",
                  "text": ""
                },
                "margin-left": "0.01*@sf"
              }
            ],
            "width": "pw"
          }
        ],
        "overflow-y": "auto",
        "size": "pw, fh"
      },
      {
        "padding": "10*@sf/@pf_outdated, 5*@sf/@pf_outdated",
        "size": "pw, 80*@sf/@pf_outdated",
        "tdiv": {
          "animated_wait_icon": {
            "background-rotation": "0",
            "pos": "0, 50%ph-50%h",
            "position": "relative"
          },
          "pos": "50%pw-50%w, 50%ph-50%h",
          "position": "absolute",
          "textAreaCentered": {
            "class": "active",
            "id": "waitText",
            "max-width": "180*@sf/@pf_outdated",
            "pos": "0, 50%ph-50%h",
            "position": "relative",
            "text": "",
            "text-align": "center"
          }
        }
      }
    ],
    "timer_interval_msec": "1000"
  }
}
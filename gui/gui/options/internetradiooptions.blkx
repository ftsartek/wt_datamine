{
  "tdiv": {
    "flow": "vertical",
    "height": "@bigOptionsHeight",
    "overflow-y": "auto",
    "pos": "0.5pw-0.5w, 0",
    "position": "relative",
    "tdiv": {
      "flow": "vertical",
      "max-height": "@bigOptionsHeight",
      "optionsBoxRightScroll": {
        "id": "internetRadioOptions"
      },
      "pos": "0.5pw-0.5w, 0.5ph-0.5h",
      "tdiv": [
        {
          "Button_text": [
            {
              "_on_click": "onDialogAddRadio",
              "text": "#options/internet_radio_add",
              "tooltip": "#guiHints/internet_radio_add"
            },
            {
              "_on_click": "onDialogEditRadio",
              "enable": "no",
              "id": "btn_edit_radio",
              "text": "#options/internet_radio_edit",
              "tooltip": "#guiHints/internet_radio_edit"
            },
            {
              "_on_click": "onRemoveRadio",
              "enable": "no",
              "id": "btn_remove_radio",
              "text": "#options/internet_radio_remove",
              "tooltip": "#guiHints/internet_radio_remove"
            }
          ],
          "pos": "50%pw- 50%w",
          "position": "relative"
        },
        {
          "flow": "vertical",
          "padding-top": "0.01@sf",
          "tdiv": [
            {
              "td": [
                {
                  "cellType": "left",
                  "optiontext": {
                    "text": "#options/internet_radio_shortcut"
                  },
                  "width": "0.5pw"
                },
                {
                  "Button_text": {
                    "_on_click": "onAssignInternetRadioButton",
                    "textarea": {
                      "id": "internet_radio_shortcut",
                      "input-transparent": "yes",
                      "min-width": "@buttonWidth-2@buttonTextPadding",
                      "pos": "0, 0.5ph-0.5h",
                      "position": "relative",
                      "text": "",
                      "text-align": "center",
                      "voiceShortcut": "yes"
                    },
                    "tooltip": "#guiHints/internet_radio_shortcut"
                  },
                  "cellType": "right",
                  "padding-left": "@optPad",
                  "width": "0.5pw"
                }
              ],
              "width": "pw"
            },
            {
              "padding-top": "0.005@sf",
              "td": [
                {
                  "cellType": "left",
                  "width": "0.5pw"
                },
                {
                  "Button_text": {
                    "_on_click": "onClearInternetRadioButton",
                    "text": "#mainmenu/btnReset",
                    "tooltip": "#mainmenu/btnReset"
                  },
                  "cellType": "right",
                  "padding-left": "@optPad",
                  "width": "0.5pw"
                }
              ],
              "width": "pw"
            }
          ],
          "width": "pw"
        },
        {
          "flow": "vertical",
          "padding-top": "0.015@sf",
          "tdiv": [
            {
              "td": [
                {
                  "cellType": "left",
                  "optiontext": {
                    "text": "#options/internet_radio_prev_shortcut"
                  },
                  "width": "0.5pw"
                },
                {
                  "Button_text": {
                    "_on_click": "onAssignInternetRadioPrevButton",
                    "textarea": {
                      "id": "internet_radio_prev_shortcut",
                      "input-transparent": "yes",
                      "min-width": "@buttonWidth-2@buttonTextPadding",
                      "pos": "0, 0.5ph-0.5h",
                      "position": "relative",
                      "text": "",
                      "text-align": "center",
                      "voiceShortcut": "yes"
                    },
                    "tooltip": "#guiHints/internet_radio_prev_shortcut"
                  },
                  "cellType": "right",
                  "padding-left": "@optPad",
                  "width": "0.5pw"
                }
              ],
              "width": "pw"
            },
            {
              "padding-top": "0.005@sf",
              "td": [
                {
                  "cellType": "left",
                  "width": "0.5pw"
                },
                {
                  "Button_text": {
                    "_on_click": "onClearInternetRadioPrevButton",
                    "text": "#mainmenu/btnReset",
                    "tooltip": "#mainmenu/btnReset"
                  },
                  "cellType": "right",
                  "padding-left": "@optPad",
                  "width": "0.5pw"
                }
              ],
              "width": "pw"
            }
          ],
          "width": "pw"
        },
        {
          "flow": "vertical",
          "padding-top": "0.015@sf",
          "tdiv": [
            {
              "td": [
                {
                  "cellType": "left",
                  "optiontext": {
                    "text": "#options/internet_radio_next_shortcut"
                  },
                  "width": "0.5pw"
                },
                {
                  "Button_text": {
                    "_on_click": "onAssignInternetRadioNextButton",
                    "textarea": {
                      "id": "internet_radio_next_shortcut",
                      "input-transparent": "yes",
                      "min-width": "@buttonWidth-2@buttonTextPadding",
                      "pos": "0, 0.5ph-0.5h",
                      "position": "relative",
                      "text": "",
                      "text-align": "center",
                      "voiceShortcut": "yes"
                    },
                    "tooltip": "#guiHints/internet_radio_next_shortcut"
                  },
                  "cellType": "right",
                  "padding-left": "@optPad",
                  "width": "0.5pw"
                }
              ],
              "width": "pw"
            },
            {
              "padding-top": "0.005@sf",
              "td": [
                {
                  "cellType": "left",
                  "width": "0.5pw"
                },
                {
                  "Button_text": {
                    "_on_click": "onClearInternetRadioNextButton",
                    "text": "#mainmenu/btnReset",
                    "tooltip": "#mainmenu/btnReset"
                  },
                  "cellType": "right",
                  "padding-left": "@optPad",
                  "width": "0.5pw"
                }
              ],
              "width": "pw"
            }
          ],
          "width": "pw"
        }
      ],
      "width": "pw"
    },
    "width": "pw"
  }
}
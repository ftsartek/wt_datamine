{
  "debriefingTab": {
    "id": "battle_log_tab",
    "tdiv": {
      "debrBlock": [
        {
          "RadioButtonList": {
            "id": "battle_log_filter",
            "navigatorShortcuts": "yes",
            "on_select": "onChangeBattlelogFilter"
          },
          "debrSeparator": {
            "class": "bottom"
          },
          "padding": "1@debrPad",
          "width": "pw"
        },
        {
          "id": "battle_log_div",
          "overflow-y": "auto",
          "scrollbarShortcuts": "yes",
          "size": "pw, fh",
          "tdiv": {
            "chatLog": {
              "id": "battle_log",
              "on_link_click": "onChatLinkClick",
              "on_link_rclick": "onChatLinkRClick",
              "style": "color:@white",
              "text": "",
              "width": "pw"
            },
            "margin": "1@debrPad",
            "min-height": "ph",
            "width": "pw"
          }
        }
      ],
      "flow": "vertical",
      "size": "pw, ph"
    }
  }
}
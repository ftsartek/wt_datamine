{
  "debriefingTab": {
    "debrBlock": {
      "id": "players_stats_scroll_block",
      "overflow-y": "auto",
      "padding": "0.5@debrPad",
      "position": "absolute",
      "scrollbarShortcuts": "yes",
      "size": "pw, ph",
      "tdiv": {
        "size": "pw, ph",
        "tdiv": {
          "height": "ph",
          "pos": "pw/2-w/2, 0",
          "position": "relative",
          "tdiv": [
            {
              "flow": "vertical",
              "id": "team1-root",
              "size": "50%p.p.w, ph",
              "tdiv": [
                {
                  "activeText": {
                    "id": "team_text",
                    "padding-right": "0.01@scrn_tgt",
                    "position": "relative",
                    "top": "50%ph-50%h"
                  },
                  "css-hier-invalidate": "yes",
                  "id": "team1_info",
                  "left": "pw-w-@tablePad",
                  "position": "relative",
                  "tdiv": {
                    "id": "countries_block",
                    "position": "relative"
                  },
                  "teamIco": "allies",
                  "teamImg": {
                    "background-svg-size": "0.04@scrn_tgt, 0.04@scrn_tgt",
                    "id": "team_img",
                    "pos": "0, 50%ph-50%h",
                    "position": "relative",
                    "size": "0.04@scrn_tgt, 0.04@scrn_tgt"
                  }
                },
                {
                  "margin-bottom": "0.0025@scrn_tgt",
                  "margin-left": "1@scrollBarSize",
                  "min-height": "0.037@scrn_tgt",
                  "table": {
                    "baseRow": "rows16",
                    "class": "headerTable",
                    "display": "hide",
                    "id": "team_table_1",
                    "pos": "0, 50%ph-50%h",
                    "position": "absolute",
                    "width": "pw"
                  },
                  "width": "pw"
                },
                {
                  "overflow-y": "auto",
                  "padding-right": "-1@scrollBarSize",
                  "scroll-align": "left",
                  "scroll-padding": "no",
                  "size": "pw, fh",
                  "table": {
                    "baseRow": "rows16",
                    "behavior": "PosNavigator",
                    "canSelectNone": "yes",
                    "class": "mpTable",
                    "css-hier-invalidate": "yes",
                    "id": "table_kills_team1",
                    "navigatorShortcuts": "yes",
                    "on_dbl_click": "onUserCard",
                    "on_hover": "onStatTblFocus",
                    "on_r_click": "onUserRClick",
                    "on_select": "onStatsTblSelect",
                    "on_unhover": "onStatTblFocus",
                    "pos": "1@scrollBarSize, 1",
                    "position": "relative",
                    "selfFocusBorder": "yes",
                    "width": "fw - 1@scrollBarSize - @tablePad"
                  }
                }
              ]
            },
            {
              "flow": "vertical",
              "id": "team2-root",
              "size": "50%p.p.w, ph",
              "tdiv": [
                {
                  "activeText": {
                    "id": "team_text",
                    "padding-left": "0.01@scrn_tgt",
                    "position": "relative",
                    "top": "50%ph-50%h"
                  },
                  "css-hier-invalidate": "yes",
                  "id": "team2_info",
                  "left": "@tablePad",
                  "position": "relative",
                  "tdiv": {
                    "id": "countries_block",
                    "position": "relative"
                  },
                  "teamIco": "axis",
                  "teamImg": {
                    "background-svg-size": "0.04@scrn_tgt, 0.04@scrn_tgt",
                    "id": "team_img",
                    "pos": "0, 50%ph-50%h",
                    "position": "relative",
                    "size": "0.04@scrn_tgt, 0.04@scrn_tgt"
                  }
                },
                {
                  "margin-bottom": "0.0025@scrn_tgt",
                  "margin-right": "1@scrollBarSize",
                  "min-height": "0.037@scrn_tgt",
                  "table": {
                    "baseRow": "rows16",
                    "class": "headerTable",
                    "display": "hide",
                    "id": "team_table_2",
                    "pos": "@tablePad, 50%ph-50%h",
                    "position": "absolute",
                    "width": "pw"
                  },
                  "width": "pw"
                },
                {
                  "overflow-y": "auto",
                  "size": "pw, fh",
                  "table": {
                    "baseRow": "rows16",
                    "behavior": "PosNavigator",
                    "canSelectNone": "yes",
                    "class": "mpTable",
                    "css-hier-invalidate": "yes",
                    "id": "table_kills_team2",
                    "navigatorShortcuts": "yes",
                    "on_dbl_click": "onUserCard",
                    "on_hover": "onStatTblFocus",
                    "on_r_click": "onUserRClick",
                    "on_select": "onStatsTblSelect",
                    "on_unhover": "onStatTblFocus",
                    "pos": "@tablePad, 1",
                    "position": "relative",
                    "selfFocusBorder": "yes",
                    "width": "fw - 1@scrollBarSize"
                  }
                }
              ]
            }
          ]
        }
      }
    },
    "id": "players_stats_tab"
  }
}
{
  "tooltipObj": {
    "behavior": "Timer",
    "display": "hide",
    "flow": "vertical",
    "id": "dmviewer_hint",
    "input-transparent": "yes",
    "movie": {
      "display": "hide",
      "id": "dmviewer_anim",
      "movie-autoStart": "yes",
      "movie-loop": "yes",
      "size": "@tooltipAnimationWidth,@tooltipAnimationHeight"
    },
    "order-popup": "yes",
    "pos": "sw/2-w/2, 70%sh-h/2",
    "position": "root",
    "style": "padding:16@sf/@pf; color-factor:192;",
    "tdiv": {
      "display": "hide",
      "flow": "vertical",
      "id": "dmviewer_ext_hint",
      "img": {
        "background-image": "",
        "id": "dmviewer_ext_hint_icon",
        "margin-top": "2@blockInterval",
        "position": "relative",
        "size": "@dIco, @dIco"
      },
      "loadingHint": {
        "behaviour": "bhvHint",
        "flow-align": "left",
        "id": "dmviewer_ext_hint_shortcut",
        "isWrapInRowAllowed": "yes",
        "isXrayHint": "yes",
        "position": "relative",
        "smallFont": "yes",
        "width": "pw"
      },
      "position": "relative",
      "rowSeparator": {
        "margin": "0, @blockInterval"
      },
      "textareaNoTab": [
        {
          "margin-top": "@blockInterval",
          "overlayTextColor": "active",
          "position": "relative",
          "text": "#xray_ext_hint/title"
        },
        {
          "hideEmptyText": "yes",
          "id": "dmviewer_ext_hint_desc",
          "margin-top": "@blockInterval",
          "position": "relative",
          "smallFont": "yes",
          "text": "",
          "width": "pw"
        }
      ],
      "total-input-transparent": "yes",
      "width": "0.5@sf"
    },
    "textareaNoTab": [
      {
        "id": "dmviewer_title",
        "input-transparent": "yes",
        "overlayTextColor": "active",
        "text": ""
      },
      {
        "hideEmptyText": "yes",
        "id": "dmviewer_desc",
        "input-transparent": "yes",
        "margin-top": "8@sf/@pf",
        "position": "relative",
        "text": ""
      }
    ],
    "timer_handler_func": "onDMViewerHintTimer"
  }
}
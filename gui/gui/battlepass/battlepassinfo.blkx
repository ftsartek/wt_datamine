{
  "flow": "vertical",
  "padding": "2@blockInterval",
  "tdiv": [
    {
      "battlePassInfoWatch": [
        {
          "id": "today_login_exp",
          "textareaNoTab": {
            "id": "text"
          }
        },
        {
          "id": "login_streak",
          "textareaNoTab": {
            "id": "text"
          }
        },
        {
          "id": "tomorow_login_exp",
          "textareaNoTab": {
            "id": "text"
          }
        },
        {
          "expProgress": {
            "activeText": {
              "id": "progress_text",
              "pos": "0.5pw-0.5w, 0.5ph-0.5h",
              "position": "absolute",
              "text": "",
              "tinyFont": "yes"
            },
            "big": "yes",
            "id": "progress_bar",
            "margin-left": "1@blockInterval",
            "pos": "0, 0.5ph-0.5h",
            "position": "relative",
            "type": "battlePassTask",
            "value": "0",
            "width": "fw"
          },
          "id": "season_tasks_progress",
          "textareaNoTab": {
            "text": "#battlePass/info/season_tasks_progress"
          }
        }
      ],
      "flow": "vertical",
      "id": "battle_pass_info",
      "overflow-y": "auto",
      "size": "pw, fh",
      "tdiv": [
        {
          "battlePassFlag": {
            "flagSize": "big",
            "margin-right": "3@blockInterval",
            "position": "relative",
            "textareaNoTab": {
              "id": "flag_text",
              "text": ""
            },
            "top": "0.5ph-0.5h"
          },
          "behaviour": "bhvUpdateByWatched",
          "id": "season_lvl",
          "size": "pw, 1@battlePassRankInfoHeight",
          "tdiv": {
            "behaviour": "bhvUpdateByWatched",
            "expProgress": {
              "activeText": {
                "id": "progress_text",
                "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                "position": "absolute",
                "tinyFont": "yes"
              },
              "big": "yes",
              "id": "progress_bar",
              "pos": "0, 0.5ph-h",
              "position": "relative",
              "type": "battlePassRank",
              "value": "0",
              "width": "pw"
            },
            "id": "level_exp",
            "size": "fw, ph"
          },
          "textareaNoTab": {
            "bigBoldFont": "yes",
            "margin": "3@blockInterval, 0",
            "position": "relative",
            "text": "#mainmenu/rank",
            "top": "1@battlePassBigFlagTextAbsPos"
          }
        },
        {
          "battlePassInfoWatch": [
            {
              "css-hier-invalidate": "yes",
              "icon": {},
              "id": "easy_daily_task_progress",
              "img": {
                "background-image": "",
                "id": "task_img"
              },
              "textareaNoTab": {
                "id": "text"
              }
            },
            {
              "css-hier-invalidate": "yes",
              "icon": {},
              "id": "medium_daily_task_progress",
              "img": {
                "id": "task_img"
              },
              "textareaNoTab": {
                "id": "text"
              }
            },
            {
              "id": "left_special_tasks_bought_count",
              "img": {
                "id": "task_img"
              },
              "textareaNoTab": {
                "id": "text"
              }
            }
          ],
          "display": "hide",
          "flow": "vertical",
          "id": "battle_tasks_info_nest",
          "tdiv": {
            "background-color": "@separatorBlockColor",
            "id": "task_timer_nest",
            "padding": "2@blockInterval, 2@dp",
            "pos": "0.5pw-0.5w, 4@blockInterval",
            "position": "relative",
            "textareaNoTab": {
              "behavior": "Timer",
              "id": "task_timer_text",
              "pos": "pw-w, 0.5ph-0.5h",
              "position": "relative",
              "smallFont": "yes",
              "text": "",
              "width": "pw-2@blockInterval"
            },
            "width": "pw + 4@blockInterval"
          },
          "width": "pw"
        }
      ]
    },
    {
      "Button_text": [
        {
          "ButtonImg": {},
          "btnName": "LT",
          "buttonGlance": {},
          "buttonWink": {},
          "display": "hide",
          "enable": "no",
          "id": "btn_warbondsShop",
          "img": {
            "background-image": "#ui/gameuiskin#wb.svg",
            "background-svg-size": "1@cIco, 1@cIco",
            "size": "1@cIco, 1@cIco"
          },
          "noMargin": "yes",
          "on_click": "onWarbondsShop",
          "position": "relative",
          "text": {
            "margin-left": "1@blockInterval",
            "text": "#mainmenu/btnOnlineShop"
          },
          "visualStyle": "secondary"
        },
        {
          "ButtonImg": {},
          "btnName": "L3",
          "buttonGlance": {},
          "buttonWink": {},
          "display": "hide",
          "enable": "no",
          "id": "btn_battleTask",
          "noMargin": "yes",
          "on_click": "onBattleTask",
          "pos": "1@blockInterval, 0",
          "position": "relative",
          "text": "#userlog/page/battletasks",
          "visualStyle": "secondary"
        }
      ],
      "padding-top": "1@blockInterval",
      "pos": "pw-w, 0",
      "position": "relative"
    }
  ]
}
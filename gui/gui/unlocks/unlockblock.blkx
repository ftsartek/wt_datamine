{
  "padding": "1@tooltipPadding",
  "tdiv": {
    "flow": "vertical",
    "tdiv": [
      {
        "div": [
          {
            "id": "award_image_sizer",
            "img": {
              "background-image": "#ui/gameuiskin#window_body_shadow",
              "display": "hide",
              "id": "award_multiplier",
              "pos": "ph-w, ph-h + 0.05ph",
              "position": "absolute",
              "textarea": {
                "caption": "yes",
                "id": "amount_text",
                "overlayTextColor": "userlog",
                "padding": "0.05ph",
                "removeParagraphIndent": "yes",
                "text": ""
              }
            },
            "layeredIconContainer": {
              "id": "award_image",
              "pos": "50%pw-50%w, 50%ph-50%h",
              "position": "absolute",
              "size": "ph, ph"
            },
            "size": "@profileUnlockIconSize, @profileUnlockIconSize"
          },
          {
            "challengeDescriptionProgress": {
              "display": "hide",
              "id": "progress",
              "max-width": "pw",
              "value": ""
            },
            "div": {
              "cardImg": {
                "background-image": "#ui/gameuiskin#favorite",
                "background-repeat": "expand",
                "id": "state_icon",
                "margin": "0, 0, 8@sf/@pf, 0",
                "pos": "-20%w, 0",
                "re-type": "9rect",
                "size": "@unlockIconSize, @unlockIconSize"
              },
              "display": "hide",
              "flow": "horizontal",
              "id": "state",
              "padding-top": "6@sf/@pf",
              "tdiv": {
                "flow": "h-flow",
                "pos": "0, ph/2-h/2",
                "position": "relative",
                "textareaNoTab": {
                  "hideEmptyText": "yes",
                  "id": "state_text",
                  "smallFont": "yes",
                  "width": "pw"
                },
                "width": "fw"
              },
              "width": "pw"
            },
            "flow": "vertical",
            "pos": "1@tooltipPadding, 0",
            "position": "relative",
            "textareaNoTab": [
              {
                "id": "award_title_text",
                "width": "pw"
              },
              {
                "id": "unlock_name",
                "overlayTextColor": "unlockHeader",
                "padding-top": "6@sf/@pf",
                "pare-text": "yes",
                "width": "pw"
              },
              {
                "id": "desc_text",
                "padding-top": "2@dp",
                "smallFont": "yes",
                "width": "pw"
              },
              {
                "id": "mainCond",
                "margin-top": "@blockInterval",
                "overflow": "hidden",
                "smallFont": "yes",
                "text": "",
                "width": "pw"
              },
              {
                "id": "multDesc",
                "overflow": "hidden",
                "smallFont": "yes",
                "text": "",
                "width": "pw"
              },
              {
                "id": "conds",
                "margin-top": "@blockInterval",
                "overflow": "hidden",
                "smallFont": "yes",
                "text": "",
                "width": "pw"
              },
              {
                "id": "obtain_info",
                "overflow": "hidden",
                "smallFont": "yes",
                "text": "",
                "width": "pw"
              },
              {
                "id": "mult_awards_text",
                "smallFont": "yes",
                "width": "pw"
              },
              {
                "id": "award_text",
                "smallFont": "yes",
                "width": "pw"
              }
            ],
            "width": "fw - 1@tooltipPadding"
          }
        ],
        "width": "pw"
      },
      {
        "display": "hide",
        "flow": "h-flow",
        "id": "subunlocks",
        "margin-top": "@blockInterval",
        "pos": "50%(pw-w), 0",
        "position": "relative",
        "width": "0.5@sf"
      }
    ],
    "width": "pw"
  }
}
{
  "animated_wait_icon": {
    "background-rotation": "0",
    "id": "stats_loading",
    "pos": "50%pw-50%w, 50%ph-50%h",
    "position": "absolute"
  },
  "flow": "vertical",
  "tdiv": [
    {
      "HorizontalListBox": {
        "id": "modes_list",
        "move-only-hover": "yes",
        "navigatorShortcuts": "yes",
        "on_select": "onStatsModeChange",
        "type": "transparent"
      },
      "flow": "horizontal",
      "padding-bottom": "1@blockInterval",
      "tdiv": {
        "EditBox": {
          "edit-hint": "#contacts/search_placeholder",
          "id": "filter_edit_box",
          "on_cancel_edit": "onFilterCancel",
          "on_change_value": "applyFilter",
          "text": "",
          "width": "fw"
        },
        "flow": "horizontal",
        "left": "pw-w",
        "popupFilter": {
          "margin-left": "1@blockInterval"
        },
        "position": "absolute",
        "width": "450@sf/@pf"
      },
      "width": "pw"
    },
    {
      "pos": "0.5pw-0.5w, 0",
      "position": [
        "relative",
        "relative"
      ],
      "size": "pw, fh",
      "table": {
        "class": "lbTable",
        "higlightRowOnHover": "yes",
        "id": "airs_stats_table",
        "pos": "0.5pw-0.5w, 0",
        "position": "relative",
        "size": "pw, ph",
        "text-halign": "center",
        "text-valign": "center"
      }
    }
  ]
}
{
  "blur": [],
  "blur_foreground": {
    "isRoot": "yes"
  },
  "div": {
    "div": [
      {
        "id": "msgTextRoot"
      },
      {
        "width": "pw"
      },
      {
        "pos": "0, ph/2-h/2"
      },
      {
        "position": "relative"
      },
      {
        "flow": "vertical"
      },
      {
        "overflow-y": "auto"
      },
      {
        "textarea": {
          "border-color": "@commonTextColor",
          "font-bold": "@fontSmall",
          "id": "msgText",
          "left": "0.5(pw-w)",
          "mediumFont": "yes",
          "overflow-y": "auto",
          "padding-left": "0.5*@scrn_tgt/100.0",
          "position": "relative",
          "text-align": "center",
          "width": "1.1@sf"
        }
      },
      {
        "div": {
          "display": "hide",
          "id": "msg_div_after_text",
          "max-height": "0.4@scrn_tgt",
          "overflow-y": "auto",
          "pos": "0.5(pw-w), 0.01@scrn_tgt",
          "position": "relative"
        }
      },
      {
        "div": {
          "display": "hide",
          "id": "msg_bottom_div",
          "pos": "0.5(pw-w), 0.01@scrn_tgt",
          "position": "relative"
        }
      },
      {
        "animated_wait_icon": {
          "background-rotation": "0",
          "display": "hide",
          "id": "msgWaitAnimation",
          "pos": "0.5(pw-w), 3*@scrn_tgt/100.0",
          "position": "relative"
        }
      },
      {
        "msgBoxButtonsHolder": {
          "behavior": "PosNavigator",
          "id": "buttons_holder",
          "navigatorShortcuts": "noSelect",
          "pos": "0.5(pw-w), 0.03@scrn_tgt",
          "position": "relative",
          "total-input-transparent": "yes"
        }
      }
    ],
    "flow": "vertical",
    "max-height": "0.9@scrn_tgt",
    "min-height": "0.5@scrn_tgt",
    "optionlist_container": {
      "text": {
        "display": "hide",
        "id": "msg_debug_string",
        "input-transparent": "yes",
        "overlayTextColor": "faded",
        "pos": "0.5(pw-w), ph +0.01@scrn_tgt",
        "position": "absolute",
        "smallFont": "yes",
        "text": ""
      },
      "type": "msg_box",
      "width": "pw"
    },
    "position": "absolute",
    "top": "0.5(ph-h)",
    "width": "pw"
  },
  "focusFrameAnimBlock": {
    "focusFrameAnimObj": []
  },
  "id": "msg_box_root",
  "menutitle": {
    "id": "msgbox-title"
  },
  "navBar": {
    "style": "id:msg-nav-bar"
  },
  "position": "absolute",
  "size": "sw, sh",
  "timer": {
    "id": "msg_box_timer",
    "timer_handler_func": "onUpdate"
  },
  "widgets": {
    "voiceChatWidgetNest": [
      {
        "isClanOnly": "no"
      },
      {
        "blockSeparator": {
          "display": "hide"
        }
      },
      {
        "blockSeparator": {
          "display": "hide"
        }
      },
      {
        "emptyVoiceChat": {
          "tooltip": "#clan/voice_chat/widget",
          "usertalk": {
            "_size-timer": "0",
            "activeText": {
              "color-factor": "255",
              "position": "relative",
              "text": "..."
            },
            "fade": "in",
            "height-end": "1",
            "id": "voice_chat_no_activity",
            "img": {
              "color-factor": "255",
              "inacive": "yes",
              "position": "relative"
            },
            "type": "noTalk"
          }
        }
      },
      {
        "voiceChatWidgetContainer": {
          "isSmall": "no",
          "voiceChatWidget": {
            "value": "{\"viewId\": \"VOICE_CHAT\"}"
          }
        }
      }
    ]
  }
}
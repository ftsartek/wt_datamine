{
  "gamercard_div": [],
  "popup_menu": [
    {
      "id": "squad_invites"
    },
    {
      "position": "root"
    },
    {
      "not-input-transparent": "yes"
    },
    {
      "css-hier-invalidate": "yes"
    },
    {
      "flow": "vertical"
    },
    {
      "menu_align": "top"
    },
    {
      "div": {
        "ComboBox": {
          "btnName": "X",
          "id": "squad_size_option",
          "on_select": "onSquadSizeChange",
          "pos": "50%pw - 50%w, 1@blockInterval",
          "position": "relative",
          "size": "pw, @buttonHeight"
        },
        "activeText": {
          "pos": "50%pw - 50%w, 0",
          "position": "relative",
          "text": "#options/squadSize",
          "text-align": "center"
        },
        "flow": "vertical",
        "id": "options_block",
        "padding-bottom": "1@blockInterval",
        "textAreaCentered": {
          "pos": "50%pw - 50%w, 1@blockInterval",
          "position": "relative",
          "smallFont": "yes",
          "text": "#guiHints/squadSize",
          "width": "pw"
        },
        "width": "0.35@sf"
      }
    },
    {
      "div": {
        "flow": "h-flow",
        "flow-align": "center",
        "id": "invites_list",
        "padding-bottom": "1@blockInterval",
        "pos": "50%pw-50%w, 0",
        "position": "relative"
      }
    },
    {
      "div": {
        "flow": "h-flow",
        "flow-align": "center",
        "id": "applications_list",
        "padding-bottom": "1@blockInterval",
        "pos": "50%pw-50%w, 0",
        "position": "relative"
      }
    },
    {
      "CheckBox": {
        "ButtonImg": [],
        "CheckBoxImg": [],
        "btnName": "Y",
        "id": "receive_applications",
        "on_change_value": "onReceiveApplications",
        "text": "#squad/receiveApplications",
        "value": "yes",
        "width": "fw"
      }
    },
    {
      "activeText": {
        "id": "invited_players_header",
        "margin": "0.01@sf, 0",
        "pos": "50%pw - 50%w, 1@blockInterval",
        "position": "relative",
        "text": "#squad/invitedPlayers",
        "text-align": "center"
      }
    },
    {
      "activeText": {
        "id": "applications_list_header",
        "margin": "0.01@sf, 0",
        "pos": "50%pw - 50%w, 1@blockInterval",
        "position": "relative",
        "text": "#squad/applications",
        "text-align": "center"
      }
    },
    {
      "popup_menu_arrow": []
    }
  ],
  "rootUnderPopupMenu": {
    "on_click": "goBack",
    "on_r_click": "goBack"
  },
  "tdiv": {
    "flow": "vertical",
    "id": "chatPopupNest",
    "pos": "1@bw + 0.5@rw-w-0.55@titleLogoPlateWidth, @topBarHeight",
    "position": "absolute",
    "size": "0.4@sf+10, 0.075*@sf+10"
  }
}
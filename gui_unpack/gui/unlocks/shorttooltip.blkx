{
  "tdiv": [
    {
      "flow": "vertical"
    },
    {
      "padding-left": "@blockInterval"
    },
    {
      "textareaNoTab": {
        "hideEmptyText": "yes",
        "id": "header",
        "max-width": "0.6@sf",
        "overlayTextColor": "unlockHeader"
      }
    },
    {
      "textareaNoTab": {
        "hideEmptyText": "yes",
        "id": "chapter",
        "max-width": "0.6@sf",
        "smallFont": "yes"
      }
    },
    {
      "textareaNoTab": {
        "hideEmptyText": "yes",
        "id": "desc_text",
        "margin-top": "2@dp",
        "max-width": "0.6@sf",
        "smallFont": "yes"
      }
    },
    {
      "textareaNoTab": {
        "hideEmptyText": "yes",
        "id": "reward",
        "margin-top": "7@dp",
        "smallFont": "yes"
      }
    },
    {
      "tdiv": {
        "cardImg": {
          "background-image": "#ui/gameuiskin#favorite",
          "display": "hide",
          "id": "challenge_complete",
          "margin-right": "@blockInterval",
          "size": "@unlockIconSize, @unlockIconSize"
        },
        "flow": "horizontal",
        "tdiv": [
          {
            "flow": "vertical"
          },
          {
            "challengeDescriptionProgress": {
              "display": "hide",
              "id": "progress"
            }
          },
          {
            "textareaNoTab": {
              "hideEmptyText": "yes",
              "id": "mainCond",
              "margin-top": "@blockInterval",
              "max-width": "0.6@sf",
              "smallFont": "yes"
            }
          },
          {
            "textareaNoTab": {
              "hideEmptyText": "yes",
              "id": "multDesc",
              "max-width": "0.6@sf",
              "smallFont": "yes"
            }
          },
          {
            "textareaNoTab": {
              "hideEmptyText": "yes",
              "id": "conds",
              "margin-top": "@blockInterval",
              "max-width": "0.6@sf",
              "smallFont": "yes"
            }
          }
        ]
      }
    },
    {
      "tdiv": {
        "display": "hide",
        "flow": "h-flow",
        "id": "subunlocks",
        "margin-top": "@blockInterval",
        "width": "0.6@sf"
      }
    }
  ]
}
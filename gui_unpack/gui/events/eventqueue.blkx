{
  "div": [
    {
      "id": "queue_box"
    },
    {
      "size": "pw, ph"
    },
    {
      "pos": "0,0"
    },
    {
      "position": "relative"
    },
    {
      "padding": "4*@sf/@pf_outdated"
    },
    {
      "flow": "vertical"
    },
    {
      "overflow-y": "auto"
    },
    {
      "behavior": "Timer"
    },
    {
      "timer_interval_msec": "1000"
    },
    {
      "tdiv": [
        {
          "size": "pw, fh"
        },
        {
          "flow": "vertical"
        },
        {
          "overflow-y": "auto"
        },
        {
          "frameBlock_dark": [
            {
              "id": "1_block"
            },
            {
              "pos": "0,0"
            },
            {
              "position": "relative"
            },
            {
              "width": "pw"
            },
            {
              "padding": "3*@sf/@pf_outdated"
            },
            {
              "flow": "vertical"
            },
            {
              "display": "hide"
            },
            {
              "css-hier-invalidate": "yes"
            },
            {
              "bgTeamColor": "blue"
            },
            {
              "activeText": {
                "id": "team_name",
                "pos": "0, 0",
                "position": "relative",
                "text": "#events/teamA"
              }
            },
            {
              "tdiv": {
                "id": "countries",
                "pos": "0, 0",
                "position": "relative",
                "width": "pw"
              }
            },
            {
              "tdiv": {
                "activeText": {
                  "id": "players_count",
                  "text": ""
                },
                "margin-left": "0.01*@sf"
              }
            },
            {
              "table": {
                "id": "table_queue_stat",
                "smallFont": "yes",
                "width": "pw"
              }
            }
          ]
        },
        {
          "frameBlock_dark": [
            {
              "id": "2_block"
            },
            {
              "pos": "0, @blockInterval"
            },
            {
              "position": "relative"
            },
            {
              "width": "pw"
            },
            {
              "padding": "3*@sf/@pf_outdated"
            },
            {
              "flow": "vertical"
            },
            {
              "display": "hide"
            },
            {
              "css-hier-invalidate": "yes"
            },
            {
              "bgTeamColor": "red"
            },
            {
              "activeText": {
                "id": "team_name",
                "pos": "0, 0",
                "position": "relative",
                "text": "#events/teamB"
              }
            },
            {
              "tdiv": {
                "id": "countries",
                "pos": "0, 0",
                "position": "relative",
                "width": "pw"
              }
            },
            {
              "tdiv": {
                "activeText": {
                  "id": "players_count",
                  "text": ""
                },
                "margin-left": "0.01*@sf"
              }
            },
            {
              "table": {
                "id": "table_queue_stat",
                "smallFont": "yes",
                "width": "pw"
              }
            }
          ]
        }
      ]
    },
    {
      "tdiv": {
        "padding": "10*@sf/@pf_outdated, 5*@sf/@pf_outdated",
        "size": "pw, 80*@sf/@pf_outdated",
        "tdiv": {
          "animated_wait_icon": {
            "background-rotation": "0",
            "pos": "0, 50%ph-50%h",
            "position": "relative"
          },
          "pos": "50%pw-50%w, 50%ph-50%h",
          "position": "absolute",
          "textAreaCentered": {
            "class": "active",
            "id": "waitText",
            "max-width": "180*@sf/@pf_outdated",
            "pos": "0, 50%ph-50%h",
            "position": "relative",
            "text": "",
            "text-align": "center"
          }
        }
      }
    }
  ]
}
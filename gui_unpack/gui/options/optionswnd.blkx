        "normalFont": "yes",
        "on_select": "onGroupSelect"
      },
      "class": "wndNav",
      "frame_header": {
        "Breadcrumb": {
          "Button_text": {
            "_on_click": "goBack",
            "btnText": {
              "id": "back_scene_name"
            },
            "img": [],
            "text": {
              "text": " | "
            },
            "textareaNoTab": {
              "text": "#mainmenu/btnSessionSettings"
            },
            "visualStyle": "noBgr"
          }
        },
        "Button_close": []
      },
      "fullScreenSize": "yes",
      "height": "sh-@bottomMenuPanelHeight",

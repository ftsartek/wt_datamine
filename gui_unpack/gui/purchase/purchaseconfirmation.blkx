[
  {
    "position": "absolute"
  },
  {
    "size": "sw, sh"
  },
  {
    "blur": []
  },
  {
    "blur_foreground": {
      "isRoot": "yes"
    }
  },
  {
    "div": {
      "div": {
        "flow": "vertical",
        "overflow-y": "auto",
        "pos": "0, ph/2-h/2",
        "position": "relative",
        "purchaseButtonsHolder": [
          {
            "id": "buttons_holder"
          },
          {
            "pos": "0.5(pw-w), 0.03@scrn_tgt"
          },
          {
            "position": "relative"
          },
          {
            "behavior": "PosNavigator"
          },
          {
            "navigatorShortcuts": "noSelect"
          },
          {
            "total-input-transparent": "yes"
          },
          {
            "Button_text": {
              "ButtonImg": [],
              "btnName": "X",
              "btnText": {
                "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                "position": "absolute",
                "text": "#msgbox/btn_yes"
              },
              "on_click": "onButtonYes",
              "selected": "yes"
            }
          },
          {
            "Button_text": {
              "ButtonImg": [],
              "btnName": "B",
              "btnText": {
                "pos": "0.5pw-0.5w, 0.5ph-0.5h",
                "position": "absolute",
                "text": "#msgbox/btn_no"
              },
              "on_click": "onButtonNo"
            }
          }
        ],
        "textarea": {
          "border-color": "@commonTextColor",
          "font-bold": "@fontSmall",
          "id": "msgText",
          "left": "0.5(pw-w)",
          "mediumFont": "yes",
          "overflow-y": "auto",
          "padding-left": "0.5*@scrn_tgt/100.0",
          "position": "relative",
          "text-align": "center",
          "width": "1.1@sf"
        },
        "width": "pw"
      },
      "flow": "vertical",
      "height": "0.5@scrn_tgt",
      "max-height": "0.9@scrn_tgt",
      "optionlist_container": {
        "type": "msg_box",
        "width": "pw"
      },
      "position": "absolute",
      "top": "0.5(ph-h)",
      "width": "pw"
    }
  },
  {
    "tdiv": {
      "gcButtonsHolder": [
        {
          "balanceText": {
            "id": "gc_warpoints",
            "img": {
              "background-image": "#ui/gameuiskin#shop_warpoints.svg"
            },
            "line": [],
            "text": {
              "id": "gc_balance"
            },
            "tooltip": "#mainmenu/warpoints"
          }
        },
        {
          "balanceText": {
            "id": "gc_eagles",
            "img": {
              "background-image": "#ui/gameuiskin#shop_warpoints_premium.svg"
            },
            "text": {
              "id": "gc_gold"
            },
            "tooltip": "#mainmenu/gold"
          }
        }
      ],
      "height": "1@gamercardHeight",
      "pos": "50%pw + 0.5@gcToBattleButtonPlaceWidth, 1@topGamercardButtonsPos",
      "position": "absolute"
    }
  },
  {
    "dummy": {
      "_on_click": "onButtonNo",
      "behavior": "accesskey",
      "btnName": "B"
    }
  },
  {
    "dummy": {
      "_on_click": "onDummyA",
      "behavior": "accesskey",
      "btnName": "A"
    }
  }
]
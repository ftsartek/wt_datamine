{
  "chapterListPlace": [
    {
      "id": "chapter_place"
    },
    {
      "height": "ph"
    },
    {
      "flow": "vertical"
    },
    {
      "tdiv": {
        "display": "hide",
        "id": "div_before_chapters_list",
        "width": "pw"
      }
    },
    {
      "tdiv": {
        "animated_wait_icon": {
          "background-rotation": "0",
          "display": "hide",
          "id": "items_list_wait_icon",
          "pos": "50%pw-50%w, 40%ph-40%h",
          "position": "absolute"
        },
        "flow": "vertical",
        "id": "chapters_list_place",
        "listbox": {
          "_on_dbl_click": "onItemDblClick",
          "flow": "vertical",
          "id": "items_list",
          "move-only-hover": "yes",
          "navigatorShortcuts": "yes",
          "on_select": "onItemSelect",
          "size": "pw, fh"
        },
        "popupFilter": {
          "margin-left": "1@buttonMargin"
        },
        "size": "pw, fh",
        "tdiv": {
          "id": "listbox_filter_holder",
          "pos": "1@blockInterval, 0",
          "position": "relative",
          "width": "pw"
        },
        "textAreaCentered": {
          "hideEmptyText": "yes",
          "id": "items_list_msg",
          "pos": "50%pw-50%w, 40%ph-40%h",
          "position": "absolute",
          "text": "",
          "width": "80%pw"
        }
      }
    }
  ],
  "chapterSeparator": {
    "margin": "1@blockInterval, 0"
  },
  "tdiv": [
    {
      "id": "item_desc"
    },
    {
      "flow": "vertical"
    },
    {
      "size": "fw, ph"
    },
    {
      "overflow-y": "auto"
    },
    {
      "total-input-transparent": "yes"
    },
    {
      "text": {
        "id": "item_name",
        "left": "(pw-w)/2",
        "position": "relative",
        "style": "font:@fontMedium;color:@titleTextColor;",
        "text": "",
        "textHide": "yes"
      }
    },
    {
      "tdiv": {
        "flow": "vertical",
        "id": "mission_desc",
        "position": "absolute",
        "size": "pw,ph"
      }
    },
    {
      "tdiv": [
        {
          "id": "chapter_include_block"
        },
        {
          "size": "pw, fh"
        },
        {
          "flow": "vertical"
        },
        {
          "display": "hide"
        },
        {
          "img": {
            "background-image": "",
            "background-repeat": "expand",
            "height": "w*128/1024",
            "id": "item_desc_header_img",
            "width": "pw"
          }
        },
        {
          "tdiv": {
            "flow": "h-flow",
            "flow-align": "center",
            "id": "div_before_text",
            "width": "pw"
          }
        },
        {
          "tdiv": {
            "flow": "vertical",
            "height": "fh",
            "overflow-y": "auto",
            "scrollbarShortcuts": "yes",
            "tdiv": {
              "display": "hide",
              "flow": "vertical",
              "id": "session_results",
              "padding-top": "0.01@scrn_tgt",
              "tdiv": {
                "flow": "vertical",
                "id": "results_table_place",
                "width": "pw"
              },
              "textarea": {
                "chapterTextAreaStyle": "yes",
                "font-bold": "@fontMedium",
                "text": "#mainmenu/sessionResults"
              },
              "width": "pw"
            },
            "textarea": {
              "chapterTextAreaStyle": "yes",
              "font-bold": "@fontMedium",
              "hideEmptyText": "yes",
              "id": "item_desc_text",
              "text": "",
              "width": "pw"
            },
            "total-input-transparent": "yes",
            "width": "pw"
          }
        }
      ]
    },
    {
      "tdiv": {
        "id": "after_desc_block",
        "width": "pw"
      }
    },
    {
      "tdiv": {
        "flow": "vertical",
        "id": "item_misc"
      }
    }
  ]
}
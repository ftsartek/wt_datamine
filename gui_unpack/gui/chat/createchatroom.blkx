{
  "root": {
    "blur": [],
    "blur_foreground": [],
    "frame": [
      {
        "id": "create_room"
      },
      {
        "size": "0.62@scrn_tgt, 0.35@scrn_tgt"
      },
      {
        "class": "wndNav"
      },
      {
        "isCenteredUnderLogo": "yes"
      },
      {
        "frame_header": {
          "Button_close": [],
          "HorizontalListBox": {
            "activeAccesskeys": "RS",
            "class": "header",
            "display": "hide",
            "enable": "no",
            "height": "ph",
            "id": "tabs_list",
            "normalFont": "yes",
            "on_select": "onTabChange"
          },
          "activeText": {
            "caption": "yes",
            "id": "caption_text",
            "text": "#chat/roomCreating",
            "textHide": "yes"
          },
          "top_right_holder": {
            "id": "header_buttons"
          }
        }
      },
      {
        "div": [
          {
            "id": "room_tab"
          },
          {
            "size": "pw, ph"
          },
          {
            "flow": "vertical"
          },
          {
            "textareaNoTab": {
              "id": "chat_room_name_text",
              "pos": "0, 1@blockInterval",
              "position": "relative",
              "text": ""
            }
          },
          {
            "textareaNoTab": {
              "pos": "0, 1@blockInterval",
              "position": "relative",
              "text": "#chat/roomPassword"
            }
          },
          {
            "EditBox": {
              "_on_activate": "onFocusPassword",
              "_on_cancel_edit": "goBack",
              "id": "room_name",
              "max-len": "15",
              "on_change_value": "onChangeRoomName",
              "position": "relative",
              "text": "",
              "tooltip": "#chat/roomName",
              "width": "pw"
            }
          },
          {
            "EditBox": {
              "_on_activate": "onCreateRoom",
              "_on_cancel_edit": "goBack",
              "id": "room_password",
              "max-len": "16",
              "position": "relative",
              "text": "",
              "width": "pw"
            }
          },
          {
            "CheckBox": {
              "ButtonImg": [],
              "CheckBoxImg": [],
              "id": "room_invitation",
              "pos": "0, 7",
              "position": "relative",
              "text": "#chat/invitationsOnly",
              "value": "no",
              "width": "pw"
            }
          },
          {
            "activeText": {
              "overlayTextColor": "warning",
              "pos": "50%pw-50%w, ph-h",
              "position": "absolute",
              "smallFont": "yes",
              "text": "#chat/createRoom/info"
            }
          }
        ]
      },
      {
        "div": [
          {
            "id": "thread_tab"
          },
          {
            "size": "pw, ph"
          },
          {
            "flow": "vertical"
          },
          {
            "textareaNoTab": {
              "id": "thread_category_header",
              "pos": "0, 1@blockInterval",
              "position": "relative",
              "text": "#chat/categoryHeader"
            }
          },
          {
            "textareaNoTab": {
              "id": "thread_title_header",
              "pos": "0, 1@blockInterval",
              "position": "relative"
            }
          },
          {
            "ComboBox": {
              "id": "categories_list",
              "position": "relative",
              "width": "pw"
            }
          },
          {
            "EditBox": {
              "_on_cancel_edit": "goBack",
              "id": "thread_title",
              "multiline": "yes",
              "on_change_value": "onChangeThreadTitle",
              "size": "pw, fh"
            }
          }
        ]
      },
      {
        "navBar": {
          "navMiddle": {
            "Button_text": {
              "ButtonImg": [],
              "_on_click": "onCreateRoom",
              "btnName": "X",
              "enable": "no",
              "id": "btn_create_room",
              "text": "#chat/create",
              "tooltip": "#chat/noRoomCreating"
            }
          }
        }
      }
    ]
  }
}
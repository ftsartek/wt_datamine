{
  "tdiv": {
    "background-color": "@minorTextColor",
    "background-image": "#ui/gameuiskin#protection_analysis_target.svg",
    "background-svg-size": "48@sf/@pf, 48@sf/@pf",
    "behavior": "Timer",
    "id": "target_cursor",
    "left": "sw/2-w/2",
    "position": "root",
    "size": "48@sf/@pf, 48@sf/@pf",
    "timer_handler_func": "onTargetingCursorTimer",
    "top": "sh/2-h/2"
  },
  "tooltipObj": [
    {
      "id": "dmviewer_hint"
    },
    {
      "behavior": "Timer"
    },
    {
      "timer_handler_func": "onDMViewerHintTimer"
    },
    {
      "position": "root"
    },
    {
      "pos": "sw/2-w/2, 70%sh-h/2"
    },
    {
      "style": "padding:16@sf/@pf; color-factor:192;"
    },
    {
      "display": "hide"
    },
    {
      "flow": "vertical"
    },
    {
      "input-transparent": "yes"
    },
    {
      "order-popup": "yes"
    },
    {
      "textareaNoTab": {
        "id": "dmviewer_title",
        "input-transparent": "yes",
        "overlayTextColor": "active",
        "text": ""
      }
    },
    {
      "textareaNoTab": {
        "hideEmptyText": "yes",
        "id": "dmviewer_desc",
        "input-transparent": "yes",
        "margin-top": "8@sf/@pf",
        "position": "relative",
        "text": ""
      }
    },
    {
      "movie": {
        "display": "hide",
        "id": "dmviewer_anim",
        "movie-autoStart": "yes",
        "movie-loop": "yes",
        "size": "@tooltipAnimationWidth,@tooltipAnimationHeight"
      }
    },
    {
      "tdiv": [
        {
          "id": "dmviewer_ext_hint"
        },
        {
          "flow": "vertical"
        },
        {
          "position": "relative"
        },
        {
          "width": "0.5@sf"
        },
        {
          "total-input-transparent": "yes"
        },
        {
          "display": "hide"
        },
        {
          "rowSeparator": {
            "margin": "0, @blockInterval"
          }
        },
        {
          "textareaNoTab": {
            "margin-top": "@blockInterval",
            "overlayTextColor": "active",
            "position": "relative",
            "text": "#xray_ext_hint/title"
          }
        },
        {
          "textareaNoTab": {
            "hideEmptyText": "yes",
            "id": "dmviewer_ext_hint_desc",
            "margin-top": "@blockInterval",
            "position": "relative",
            "smallFont": "yes",
            "text": "",
            "width": "pw"
          }
        },
        {
          "img": {
            "background-image": "",
            "id": "dmviewer_ext_hint_icon",
            "margin-top": "2@blockInterval",
            "position": "relative",
            "size": "@dIco, @dIco"
          }
        },
        {
          "loadingHint": {
            "behaviour": "bhvHint",
            "flow-align": "left",
            "id": "dmviewer_ext_hint_shortcut",
            "isWrapInRowAllowed": "yes",
            "isXrayHint": "yes",
            "position": "relative",
            "smallFont": "yes",
            "width": "pw"
          }
        }
      ]
    }
  ]
}
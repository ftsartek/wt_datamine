      "range": 10000.0,
      "range0": 0.0,
      "range1": 100000.0,
      "range2": 0.0,
      "range3": 0.0,
      "range4": 0.0,
      "range5": 0.0,
      "range6": 0.0,
      "range7": 0.0,
      "rangeMax": 10000.0,
      "rcs": 3000.0,
      "visibilityType": "infraRed"
    }
  },
  "type": "radar"
}
{
  "fsms": {
    "lock": {
      "stateInit": "lock",
      "transitions": {
        "detect": {
          "actions": {
            "detectTarget": {}
          },
          "event": "targetInSight"
        },
        "scan": {
          "actions": {
            "scan": {}
          },
          "event": "update"
        }
      }
    },
    "main": {
      "actionsTemplates": {
        "init": {
          "setEnabled": {
            "value": false
          },
          "setSignal": {
            "signal": "track"
          },
          "setTargetDesignationRange": {
            "azimuthRange": [-0.5,0.5],
            "distanceRange": [0.0,10000.0]
          },
          "setTransiver": {
            "transiver": "track"
          }
        },
        "setLosLockMode": {
          "setDistGatePos": {
            "pos": 5000.0,
            "source": "constRange",
            "width": 10000.0
          },
          "setEnabled": {
            "value": true
          },
          "setModeName": {
            "name": "acquisition"
          },
          "setWeaponDetectionAllowance": {
            "source": "targetDesignation"
          }
        },
        "setLosVisualPreLockMode": {
          "clearTargetsOfInterest": {},
          "resetScanPhase": {},
          "setCenterAzimuth": {
            "source": "targetDesignation"
          },
          "setCenterElevation": {
            "source": "targetDesignation"
          },
          "setEnabled": {
            "value": true
          },
          "setFsmActive": {
            "active": true,
            "fsm": "lock"
          },
          "setScanPattern": {
            "scanPattern": "losLock"
          }
        },
        "setStandbyMode": {
          "clearTargetsOfInterest": {},
          "setCenterAzimuth": {
            "source": "constant",
            "value": 0.0
          },
          "setEnabled": {
            "value": false
          },
          "setFsmActive": [
            {
              "active": false,
              "fsm": "lock"
            },
            {
              "active": false,
              "fsm": "track"
            }
          ],
          "setModeName": {
            "name": "standby"
          },
          "setScanPattern": {
            "scanPattern": "losLock"
          }
        },
        "setTrackMode": {
          "addTargetOfInterest": {},
          "clearTargetsOfInterest": {},
          "setCenterAzimuth": {
            "source": "activeTargetOfInterest"
          },
          "setCenterElevation": {
            "source": "activeTargetOfInterest"
          },
          "setDistGatePos": {
            "source": "activeTargetOfInterest",
            "width": 0.0
          },
          "setEnabled": {
            "value": true
          },
          "setFsmActive": {
            "active": true,
            "fsm": "track"
          },
          "setLastTargetOfInterestActive": {},
          "setModeName": {
            "name": "track"
          },
          "setRelSpeedGatePos": {
            "source": "activeTargetOfInterest",
            "width": 0.0
          },
          "setScanPattern": {
            "scanPattern": "track"
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          },
          "updateActiveTargetOfInterest": {}
        },
        "setVisualLockMode": {
          "setDistGatePos": {
            "source": "targetDesignation",
            "width": 100.0
          },
          "setEnabled": {
            "value": true
          },
          "setModeName": {
            "name": "acquisition"
          },
          "setWeaponDetectionAllowance": {
            "source": "const",
            "value": true
          }
        }
      },
      "stateInit": "init",
      "transitions": {
        "checkLockType1": {
          "actions": {
            "checkDesignationTypeEquals": {
              "value": 1
            }
          },
          "command": "designateTarget",
          "stateFrom": "standby",
          "stateTo": "checkLockType1"
        },
        "checkLockType2": {
          "actions": {
            "checkDesignationTypeEquals": {
              "value": 2
            }
          },
          "event": "designationTypeNotEquals",
          "stateFrom": "checkLockType1",
          "stateTo": "checkLockType2"
        },
        "failedLock": {
          "actions": {
            "setFsmActive": {
              "active": false,
              "fsm": "lock"
            },
            "setStandbyMode": {}
          },
          "event": "scanFinished",
          "stateFrom": [
            "losLock",
            "visualLock"
          ],
          "stateTo": "standby"
        },
        "finishLockTypeCheck": {
          "event": "designationTypeNotEquals",
          "stateFrom": "checkLockType2",
          "stateTo": "standby"
        },
        "finishedTrack": {
          "actions": {
            "setFsmActive": {
              "active": false,
              "fsm": "track"
            },
            "setStandbyMode": {}
          },
          "command": [
            "selectTarget",
            "designateTarget"
          ],
          "event": "targetsOfInterestCleanup",
          "stateFrom": "track",
          "stateTo": "standby"
        },
        "init": {
          "actions": {
            "init": {},
            "setStandbyMode": {}
          },
          "event": "init",
          "stateFrom": "init",
          "stateTo": "standby"
        },
        "losLock": {
          "actions": {
            "setLosLockMode": {},
            "setLosVisualPreLockMode": {}
          },
          "event": "designationTypeEquals",
          "stateFrom": "checkLockType1",
          "stateTo": "losLock"
        },
        "track": {
          "actions": {
            "setFsmActive": {
              "active": false,
              "fsm": "lock"
            },
            "setTrackMode": {}
          },
          "event": "targetDetected",
          "stateFrom": [
            "losLock",
            "visualLock"
          ],
          "stateTo": "track"
        },
        "visualLock": {
          "actions": {
            "setLosVisualPreLockMode": {},
            "setVisualLockMode": {}
          },
          "event": "designationTypeEquals",
          "stateFrom": "checkLockType2",
          "stateTo": "visualLock"
        }
      }
    },
    "track": {
      "stateInit": "track",
      "transitions": {
        "detect": {
          "actions": {
            "detectTarget": {}
          },
          "event": "update"
        },
        "extrapolate": {
          "actions": {
            "clearTargetsOfInterest": {
              "timeOut": 2.0
            },
            "extrapolateTargetsOfInterest": {},
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            },
            "setRelSpeedGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            }
          },
          "event": "targetNotDetected"
        },
        "track": {
          "actions": {
            "setCenterAzimuth": {
              "source": "activeTargetOfInterest"
            },
            "setCenterElevation": {
              "source": "activeTargetOfInterest"
            },
            "setDistGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            },
            "setRelSpeedGatePos": {
              "source": "activeTargetOfInterest",
              "width": 0.0
            },
            "updateActiveTargetOfInterest": {
              "dirFilter": {
                "limTimeOut": 1.0
              },
              "distFilter": {
                "limTimeOut": 1.0
              },
              "dopplerSpeedFilter": {
                "limTimeOut": 1.0
              }
            }
          },
          "event": "targetDetected"
        }
      }
    }
  },
  "name": "LAV-AD",
  "scanPatterns": {
    "losLock": {
      "azimuthLimits": [-60.0,60.0],
      "barHeight": 0.5,
      "barsCount": 1,
      "elevationLimits": [-10.0,85.0],
      "indicate": true,
      "period": 0.25,
      "pitchStabLimit": 85.0,
      "rollStabLimit": 10.0,
      "rowMajor": false,
      "type": "pyramide",
      "width": 0.25
    },
    "track": {
      "azimuthLimits": [-60.0,60.0],
      "elevationLimits": [-20.0,85.0],
      "pitchStabLimit": 85.0,
      "rollStabLimit": 10.0,
      "type": "no"
    }
  },
  "signals": {
    "track": {
      "absDopplerSpeed": true,
      "angularAccuracy": 0.002,
      "distance": {
        "maxValue": 10000.0,
        "minValue": 20.0,
        "presents": true
      },
      "distanceAccuracy": 2.0,
      "groundClutter": false,
      "showBScope": false

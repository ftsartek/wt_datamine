{
  "selector": [
    {
      "name": "HLBCapture"
    },
    {
      "selectStrategy": "ordered"
    },
    {
      "sequencer": [
        {
          "listCleaner": {
            "fromList": "assaultPlanes",
            "originalList": "unsortedPlanes"
          }
        },
        {
          "listCleaner": {
            "fromList": "capturePlanes",
            "originalList": "unsortedPlanes"
          }
        },
        {
          "listCleaner": {
            "fromList": "fightingPlanes",
            "originalList": "unsortedPlanes"
          }
        },
        {
          "listCleaner": {
            "fromList": "otherPlanes",
            "originalList": "unsortedPlanes"
          }
        }
      ]
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "assaultPlanes",
          "varName": "assaulter"
        },
        "findPlaneInListCapableOf": {
          "capableOf": "bombing",
          "listName": "unsortedPlanes",
          "planeName": "assaulter"
        },
        "removeFromList": {
          "listName": "unsortedPlanes",
          "varName": "assaulter"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "assaultBot",
          "planeName": "assaulter"
        }
      }
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "assaultPlanes",
          "varName": "assaulter"
        },
        "findPlaneInListCapableOf": {
          "capableOf": "torpedoing",
          "listName": "unsortedPlanes",
          "planeName": "assaulter"
        },
        "removeFromList": {
          "listName": "unsortedPlanes",
          "varName": "assaulter"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "assaultBot",
          "planeName": "assaulter"
        }
      }
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "assaultPlanes",
          "varName": "assaulter"
        },
        "findPlaneInListCapableOf": {
          "capableOf": "rocket_attack",
          "listName": "unsortedPlanes",
          "planeName": "assaulter"
        },
        "removeFromList": {
          "listName": "unsortedPlanes",
          "varName": "assaulter"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "assaultBot",
          "planeName": "assaulter"
        }
      }
    },
    {
      "sequencer": [
        {
          "repeat": true
        },
        {
          "findCaptureZoneHighLevel": {
            "afterName": "enemyCZAfter",
            "czName": "enemyCZ",
            "friendly": false
          }
        },
        {
          "setVarTo": {
            "val": -1,
            "varName": "capturerAfter"
          }
        },
        {
          "setVarTo": {
            "val": 0,
            "varName": "alreadyCapturing"
          }
        },
        {
          "setVarTo": {
            "fromVar": "enemyCZ",
            "varName": "enemyCZAfter"
          }
        },
        {
          "sequencer": {
            "getFromList": {
              "afterName": "capturerAfter",
              "indexType": "first",
              "listName": "capturePlanes",
              "varName": "capturer"
            },
            "getVarIndex": {
              "listName": "capturePlanes",
              "toName": "capturerAfter",
              "varName": "capturer"
            },
            "ifThenElse": {
              "continue": [],
              "isBotVarEqual": {
                "botVarName": "zoneToCapture",
                "planeName": "capturer",
                "varName": "enemyCZ"
              },
              "not": {
                "setVarTo": {
                  "val": 1,
                  "varName": "alreadyCapturing"
                }
              }
            },
            "repeat": true
          }
        },
        {
          "ifThenElse": {
            "addToList": {
              "listName": "zonesToCapture",
              "varName": "enemyCZ"
            },
            "compareVar": {
              "op": "equal",
              "value": 0,
              "varName": "alreadyCapturing"
            },
            "continue": []
          }
        }
      ]
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "zonesToDefend",
          "varName": "friendlyCZ"
        },
        "findCaptureZoneHighLevel": {
          "afterName": "friendlyCZAfter",
          "czName": "friendlyCZ",
          "friendly": true
        },
        "repeat": true,
        "setVarTo": {
          "fromVar": "friendlyCZ",
          "varName": "friendlyCZAfter"
        }
      }
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "fightingPlanes",
          "varName": "fighter"
        },
        "findPlaneInListCapableOf": {
          "capableOf": "dog_fight",
          "listName": "unsortedPlanes",
          "planeName": "fighter"
        },
        "removeFromList": {
          "listName": "unsortedPlanes",
          "varName": "fighter"
        },
        "repeat": true
      }
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "assaultPlanes",
          "varName": "assaulter"
        },
        "findPlaneInListCapableOf": {
          "capableOf": "cannon_attack",
          "listName": "unsortedPlanes",
          "planeName": "assaulter"
        },
        "removeFromList": {
          "listName": "unsortedPlanes",
          "varName": "assaulter"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "assaultBot",
          "planeName": "assaulter"
        }
      }
    },
    {
      "sequencer": [
        {
          "repeat": true
        },
        {
          "getFromList": {
            "indexType": "random",
            "listName": "fightingPlanes",
            "varName": "capturer"
          }
        },
        {
          "getFromList": {
            "indexType": "random",
            "listName": "fightingPlanes",
            "varName": "assister"
          }
        },
        {
          "getClosestCZToPlane": {
            "listName": "zonesToCapture",
            "planeName": "capturer",
            "varName": "closestZone"
          }
        },
        {
          "removeFromList": {
            "listName": "zonesToCapture",
            "varName": "closestZone"
          }
        },
        {
          "removeFromList": {
            "listName": "fightingPlanes",
            "varName": "capturer"
          }
        },
        {
          "removeFromList": {
            "listName": "fightingPlanes",
            "varName": "assister"
          }
        },
        {
          "addToList": {
            "listName": "capturePlanes",
            "varName": "capturer"
          }
        },
        {
          "setBehaviourToPlane": {
            "behaviour": "captureBot",
            "planeName": "capturer"
          }
        },
        {
          "setBehaviourToPlane": {
            "behaviour": "assistCaptureBot",
            "planeName": "assister"
          }
        },
        {
          "setBotVarTo": {
            "fromVar": "closestZone",
            "planeName": "capturer",
            "toVar": "zoneToCapture"
          }
        },
        {
          "setBotVarTo": {
            "fromVar": "capturer",
            "planeName": "assister",
            "toVar": "planeToAssist"
          }
        }
      ]
    },
    {
      "sequencer": [
        {
          "repeat": true
        },
        {
          "getFromList": {
            "indexType": "random",
            "listName": "fightingPlanes",
            "varName": "defender"
          }
        },
        {
          "getClosestCZToPlane": {
            "listName": "zonesToDefend",
            "planeName": "defender",
            "varName": "closestZone"
          }
        },
        {
          "removeFromList": {
            "listName": "zonesToDefend",
            "varName": "closestZone"
          }
        },
        {
          "removeFromList": {
            "listName": "fightingPlanes",
            "varName": "defender"
          }
        },
        {
          "setBehaviourToPlane": {
            "behaviour": "defenderBot",
            "planeName": "defender"
          }
        },
        {
          "setBotVarTo": {
            "fromVar": "closestZone",
            "planeName": "defender",
            "toVar": "zoneToDefend"
          }
        }
      ]
    },
    {
      "sequencer": {
        "findPlaneInListCapableOf": {
          "capableOf": "fighting",
          "listName": "fightingPlanes",
          "planeName": "seeker"
        },
        "ifThenElse": [
          {
            "compareVar": {
              "op": "equal",
              "value": 1,
              "varName": "missionRank"
            }
          },
          {
            "setBehaviourToPlane": {
              "behaviour": "defaultCTABot",
              "planeName": "seeker"
            }
          },
          {
            "setBehaviourToPlane": {
              "behaviour": "seekerBot",
              "planeName": "seeker"
            }
          }
        ],
        "removeFromList": {
          "listName": "fightingPlanes",
          "varName": "seeker"
        },
        "repeat": true,
        "setMissionRank": {
          "varName": "missionRank"
        }
      }
    },
    {
      "sequencer": {
        "findPlaneInListCapableOf": {
          "capableOf": "cannon_attack",
          "listName": "fightingPlanes",
          "planeName": "assaulter"
        },
        "removeFromList": {
          "listName": "fightingPlanes",
          "varName": "assaulter"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "assaultBot",
          "planeName": "assaulter"
        }
      }
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "otherPlanes",
          "varName": "other"
        },
        "findPlaneInListCapableOf": {
          "capableOf": "rearming",
          "listName": "fightingPlanes",
          "planeName": "other"
        },
        "removeFromList": {
          "listName": "fightingPlanes",
          "varName": "other"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "rearmingBot",
          "planeName": "other"
        }
      }
    },
    {
      "sequencer": {
        "addToList": {
          "listName": "otherPlanes",
          "varName": "other"
        },
        "getFromList": {
          "indexType": "last",
          "listName": "unsortedPlanes",
          "varName": "other"
        },
        "removeFromList": {
          "listName": "unsortedPlanes",
          "varName": "other"
        },
        "repeat": true,
        "setBehaviourToPlane": {
          "behaviour": "rearmingBot",
          "planeName": "other"
        }
      }
    },
    {
      "clearIdList": {
        "listName": "zonesToCapture"
      }
    },
    {
      "clearIdList": {
        "listName": "zonesToDefend"
      }
    },
    {
      "setVarTo": {
        "val": -1,
        "varName": "enemyCZAfter"
      }
    },
    {
      "setVarTo": {
        "val": -1,
        "varName": "friendlyCZAfter"
      }
    }
  ]
}